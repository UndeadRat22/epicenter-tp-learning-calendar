{
  "name": "epicenter-learning-calendar",
  "version": "1.0.0",
  "scripts": {
    "start": "env PORT=8080 yoshi start",
    "build": "yoshi build",
    "test": "yoshi test",
    "lint": "yoshi lint",
    "posttest": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,scss,less}": "yoshi lint --fix"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "husky": "^4.2.1",
    "jest-yoshi-preset": "^4.1.0",
    "lint-staged": "^10.0.7",
    "velocity": "~0.7.0",
    "yoshi": "^4.1.0",
    "yoshi-style-dependencies": "^4.1.0"
  },
  "dependencies": {
    "prop-types": "~15.6.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "wix-style-react": "^8.0.0",
    "wix-ui-icons-common": "^2.0.146"
  },
  "yoshi": {
    "projectType": "app",
    "externals": {
      "react": "React",
      "react-dom": "ReactDOM"
    },
    "hmr": "auto",
    "servers": {
      "cdn": {
        "port": 3200
      }
    },
    "startUrl": "http://localhost:8080"
  },
  "jest": {
    "preset": "jest-yoshi-preset"
  },
  "eslintConfig": {
    "extends": "yoshi"
  }
}
